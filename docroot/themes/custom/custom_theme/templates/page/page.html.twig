<!-- Begin - wrapper -->
<div class="layout__wrapper">

  {% if page.sidr_left__primary or page.sidr_left__secondary %}

    <!-- Begin - sidr source provider -->
    <aside class="sidr-source-provider">

      <!-- Begin - navigation -->
      <nav class="slinky-menu" role="navigation">

        {% if page.sidr_left__primary %}
          {{ page.sidr_left__primary }}
        {% endif %}

        {% if page.sidr_left__secondary %}
          {{ page.sidr_left__secondary }}
        {% endif %}

      </nav>
      <!-- End - navigation -->

    </aside>
    <!-- End - sidr source provider -->

  {% endif %}

  <!-- Begin - header static -->
  <header class="flexy-header flexy-header--static">
    <div class="flexy-header__row flexy-header__row--first">
      <div class="container">
        <div class="flexy-row">

          <!-- Begin - logo -->
          <a href="{{ url('<front>') }}" class="flexy-header__logo">
            <img src="data:image/svg+xml;base64,">
          </a>
          <!-- End - logo -->

            <div class="flexy-spacer"></div>

            {% if page.flexy_navigation__primary %}
                <!-- Begin - navigation -->
                <nav class="flexy-header__navigation__wrapper hidden-xs hidden-sm"
                     role="navigation">
                    {{ page.flexy_navigation__primary }}
                </nav>
                <!-- End - navigation -->
            {% endif %}

          <!-- Begin - responsive toggle -->
          <button
            class="flexy-header__sidebar-toggle sidr-toggle--right visible-xs visible-sm">
            <span class="icon fa fa-bars"></span>
          </button>
          <!-- End - responsive toggle -->

        </div>
      </div>
    </div>
  </header>
  <!-- End - header static -->

  <!-- Begin - header sticky -->
  <header class="flexy-header flexy-header--sticky">
    <div class="flexy-header__row">
      <div class="container">
        <div class="flexy-row">

          <!-- Begin - logo -->
          <a href="{{ path('<front>') }}" class="flexy-header__logo">
            <img src="data:image/svg+xml;base64,">
          </a>
          <!-- End - logo -->

          <div class="flexy-spacer"></div>

          {% if page.flexy_navigation__primary %}
            <!-- Begin - navigation -->
            <nav class="flexy-header__navigation__wrapper hidden-xs hidden-sm"
                 role="navigation">
              {{ page.flexy_navigation__primary }}
            </nav>
            <!-- End - navigation -->
          {% endif %}

          <!-- Begin - responsive toggle -->
          <button
            class="flexy-header__sidebar-toggle sidr-toggle--right visible-xs visible-sm">
            <span class="icon fa fa-bars"></span>
          </button>
          <!-- End - responsive toggle -->

        </div>
      </div>
    </div>
  </header>
  <!-- End - header sticky -->

  <!-- Begin - content -->
  <main class="layout__content" role="main">

    {# Begin - banner #}
    {% if banner %}
      <div class="banner banner--{{ banner.type }} banner--text-color-{{ banner.text_color }}" style="background-image: url({{ banner.background_image }})">
        <div class="banner__inner">
          <div class="container">

            {# Begin - heading #}
            {% if banner.field_banner_heading %}
              <div class="banner__heading">
                <h2 class="banner__heading__title heading-h1">
                  {{ banner.field_banner_heading }}
                </h2>
              </div>
            {% endif %}
            {# End - heading #}

            {# Begin - body #}
            {% if banner.field_banner_body %}
              <div class="banner__body heading-h4">
                {{ banner.field_banner_body }}
              </div>
            {% endif %}
            {# End - body #}

          </div>
        </div>
      </div>
    {% endif %}
    {# End - banner #}

    <div class="container">

      {# Highlighted #}
      {% if page.highlighted %}
        {% block highlighted %}
          <div class="highlighted">
            {{ page.highlighted }}
          </div>
        {% endblock %}
      {% endif %}

      {# Help #}
      {% if page.help %}
        {% block help %}
          {{ page.help }}
        {% endblock %}
      {% endif %}

      {# Content #}
      {% block content %}
        <!-- Begin - content -->
        <a id="main-content"></a>

        {{ page.content }}
        <!-- End - content -->
      {% endblock %}

    </div>
  </main>
  <!-- End - content -->

  <!-- Begin - footer -->
  <footer class="layout__footer">
    <div class="layout__footer__inner">
      <div class="container">
        <div class="row">
          <div class="col-xs-12 col-sm-4 col-md-3">

            <!-- Begin - contact information -->
            <div class="layout__footer__contact-information">
              <div class="partial">
                <div class="partial__body">

                  <!-- Begin - logo -->
                  <div class="layout__footer__logo">
                    <img src="{{ base_path ~ directory }}/dist/images/logo.svg">
                  </div>
                  <!-- End - logo -->

                  <!-- Begin - contact information -->
                  <div class="layout__footer__contact-information">

                    {% if theme_settings.contact_information.email %}
                      <div class="layout__footer__contact-information__item layout__footer__contact-information__item--email">
                        <a
                          href="mailto: {{ theme_settings.contact_information.email }}">
                          {{ theme_settings.contact_information.email }}
                        </a>
                      </div>
                    {% endif %}

                    {% if theme_settings.contact_information.address %}
                      <div class="layout__footer__contact-information__item layout__footer__contact-information__item--address">
                        {{ theme_settings.contact_information.address }}
                      </div>
                    {% endif %}

                    {% if theme_settings.contact_information.zipcode and theme_settings.contact_information.city %}
                      <div class="layout__footer__contact-information__item layout__footer__contact-information__item--city-and-zip">
                        {{ theme_settings.contact_information.zipcode }} {{ theme_settings.contact_information.city }}
                      </div>
                    {% endif %}

                    {% if theme_settings.contact_information.phone_system and theme_settings.contact_information.phone_readable %}
                      <div class="layout__footer__contact-information__item layout__footer__contact-information__item--phone">
                        <a
                          href="tel:{{ theme_settings.contact_information.phone_system }}">Tlf.: {{ theme_settings.contact_information.phone_readable }}
                        </a>
                      </div>
                    {% endif %}

                    {% if theme_settings.contact_information.working_hours %}
                      <div class="layout__footer__contact-information__item layout__footer__contact-information__item--working-hours">
                        {{ theme_settings.contact_information.working_hours }}
                      </div>
                    {% endif %}

                  </div>
                  <!-- End - contact information -->

                  <!-- Begin - social icons -->
                  <nav class="layout__footer__social-icons"
                       role="navigation">
                    <ul class="list-inline">

                      {% if theme_settings.social.facebook.active %}
                        <li>
                          <a
                            href="{{ theme_settings.social.facebook.url }}"
                            target="_blank"
                            data-toggle="tooltip"
                            data-placement="top"
                            class="social-icon social-icon--facebook"
                            title="{{ theme_settings.social.facebook.tooltip }}">
                            <span class="icon fa fa-facebook-f"></span>
                          </a>
                        </li>
                      {% endif %}

                      {% if theme_settings.social.twitter.active %}
                        <li>
                          <a
                            href="{{ theme_settings.social.twitter.url }}"
                            target="_blank"
                            data-toggle="tooltip"
                            data-placement="top"
                            class="social-icon social-icon--twitter"
                            title="{{ theme_settings.social.twitter.tooltip }}">
                            <span class="icon fa fa-twitter"></span>
                          </a>
                        </li>
                      {% endif %}

                      {% if theme_settings.social.googleplus.active %}
                        <li>
                          <a
                            href="{{ theme_settings.social.googleplus.url }}"
                            target="_blank"
                            data-toggle="tooltip"
                            data-placement="top"
                            class="social-icon social-icon--google-plus"
                            title="{{ theme_settings.social.googleplus.tooltip }}">
                            <span class="icon fa fa-google-plus"></span>
                          </a>
                        </li>
                      {% endif %}

                      {% if theme_settings.social.linkedin.active %}
                        <li>
                          <a
                            href="{{ theme_settings.social.linkedin.url }}"
                            target="_blank"
                            data-toggle="tooltip"
                            data-placement="top"
                            class="social-icon social-icon--linkedin"
                            title="{{ theme_settings.social.linkedin.tooltip }}">
                            <span class="icon fa fa-linkedin"></span>
                          </a>
                        </li>
                      {% endif %}

                      {% if theme_settings.social.pinterest.active %}
                        <li>
                          <a
                            href="{{ theme_settings.social.pinterest.url }}"
                            target="_blank"
                            data-toggle="tooltip"
                            data-placement="top"
                            class="social-icon social-icon--pinterest"
                            title="{{ theme_settings.social.pinterest.tooltip }}">
                            <span class="icon fa fa-pinterest"></span>
                          </a>
                        </li>
                      {% endif %}

                      {% if theme_settings.social.instagram.active %}
                        <li>
                          <a
                            href="{{ theme_settings.social.instagram.url }}"
                            target="_blank"
                            data-toggle="tooltip"
                            data-placement="top"
                            class="social-icon social-icon--instagram"
                            title="{{ theme_settings.social.instagram.tooltip }}">
                            <span class="icon fa fa-instagram"></span>
                          </a>
                        </li>
                      {% endif %}

                      {% if theme_settings.social.youtube.active %}
                        <li>
                          <a
                            href="{{ theme_settings.social.youtube.url }}"
                            target="_blank"
                            data-toggle="tooltip"
                            data-placement="top"
                            class="social-icon social-icon--youtube"
                            title="{{ theme_settings.social.youtube.tooltip }}">
                            <span class="icon fa fa-youtube"></span>
                          </a>
                        </li>
                      {% endif %}

                      {% if theme_settings.social.vimeo.active %}
                        <li>
                          <a
                            href="{{ theme_settings.social.vimeo.url }}"
                            target="_blank"
                            data-toggle="tooltip"
                            data-placement="top"
                            class="social-icon social-icon--vimeo"
                            title="{{ theme_settings.social.vimeo.tooltip }}">
                            <span class="icon fa fa-vimeo"></span>
                          </a>
                        </li>
                      {% endif %}

                      {% if theme_settings.social.github.active %}
                        <li>
                          <a
                            href="{{ theme_settings.social.github.url }}"
                            target="_blank"
                            data-toggle="tooltip"
                            data-placement="top"
                            class="social-icon social-icon--github"
                            title="{{ theme_settings.social.github.tooltip }}">
                            <span class="icon fa fa-github"></span>
                          </a>
                        </li>
                      {% endif %}

                    </ul>
                  </nav>
                  <!-- End - social icons -->

                </div>
              </div>

            </div>
            <!-- End - contact information -->

          </div>
          <div class="col-xs-12 col-sm-4 col-md-3 hidden-xs">

            {% if page.footer__menu__first %}
              <!-- Begin - menu (first) -->
              <div class="layout__footer__menu layout__footer__menu--first">
                {{ page.footer__menu__first }}
              </div>
              <!-- End - menu (first) -->
            {% endif %}

          </div>
          <div class="col-xs-12 col-sm-4 col-md-3 hidden-xs">

            {% if page.footer__menu__second %}
              <!-- Begin - menu (second) -->
              <div class="layout__footer__menu layout__footer__menu--second">
                {{ page.footer__menu__second }}
              </div>
              <!-- End - menu (second) -->
            {% endif %}

          </div>
        </div>

      </div>
    </div>
  </footer>
  <!-- End - footer -->

</div>
<!-- End - wrapper -->
